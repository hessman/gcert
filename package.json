{
  "name": "gsub",
  "version": "0.1.0",
  "description": "Tool to retrieve SSL/TLS certificate reports information from the Google Transparency Report for a given domain.",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rm -rf dist .eslintcache *.tsbuildinfo",
    "build": "tsc -b .",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node dist/index.js"
  },
  "keywords": ["google", "recon", "osint", "domain", "subdomain", "certificate"],
  "author": "hessman",
  "license": "MPL-2.0",
  "dependencies": {
    "@types/cli-progress": "^3.9.2",
    "axios": "^0.23.0",
    "commander": "^8.3.0",
    "csv-stringify": "^5.6.5",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.10.3"
  }
}