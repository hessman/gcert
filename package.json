{
  "name": "gcert",
  "version": "0.4.0",
  "description": "Retrieves SSL/TLS certificate reports information from the Google Transparency Report for a given domain.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rm -rf dist .eslintcache *.tsbuildinfo",
    "build": "tsc -b .",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node dist/index.js"
  },
  "keywords": [
    "google",
    "recon",
    "osint",
    "domain",
    "subdomain",
    "certificate"
  ],
  "author": "hessman",
  "license": "MPL-2.0",
  "dependencies": {
    "axios": "^0.23.0",
    "commander": "^8.3.0",
    "csv-stringify": "^5.6.5",
    "psl": "^1.8.0",
    "pug": "^3.0.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/psl": "^1.1.0",
    "@types/pug": "^2.0.5",
    "typescript": "^4.4.4"
  }
}
